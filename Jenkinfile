pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Testing is completed'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deployment is completed'
            }
        }
            stage('Result') {
            steps {
                echo 'Deployment results shared'
            }
        }
        }
    }

